{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-2",
        "profile_name": "default",
        "project_name": "roshakserv",
        "runtime": "python3.6",
        "s3_bucket": "roshak",
        "events": [{
           "function": "app.app", // The function to execute
           "expression": "rate(5 minutes)" // When to execute it (in cron or rate format)
       }]
    }
}